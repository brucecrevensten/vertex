<script type="text/javascript" src="[% c.uri_for('/static/js/AsfConfig.js') %]"></script>
<script type="text/javascript" src="[% c.uri_for('/static/js/jquery-1.6.1.min.js') %]"></script>
<script type="text/javascript" src="[% c.uri_for('/static/js/jquery-ui-1.8.13.custom.min.js') %]"></script>
<script type="text/javascript" src="[% c.uri_for('/static/js/jquery.dataTables.min.js') %]"></script>
<script type="text/javascript" src="[% c.uri_for('/static/js/underscore-min.js') %]"></script>
<script type="text/javascript" src="[% c.uri_for('/static/js/backbone-min.js') %]"></script>
<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=true"></script>
<script type="text/javascript" src="[% c.uri_for('/static/js/SearchMap.js') %]"></script>
<script type="text/javascript" src="[% c.uri_for('/static/js/DataProduct.js') %]"></script>
<script type="text/javascript" src="[% c.uri_for('/static/js/Search.js') %]"></script>
<script type="text/javascript" src="[% c.uri_for('/static/js/SearchResults.js') %]"></script>
<script type="text/javascript" src="[% c.uri_for('/static/js/SearchApp.js') %]"></script>
<script type="text/javascript" src="[% c.uri_for('/static/js/DownloadQueue.js') %]"></script>

</head>
<body>
<div id="wrapper">
<header>
<h1><span>ASF DataPortal</span></h1>
<nav>
<ul>
<li>Home</li>
</ul>
</nav>
<div id="queue_summary" style="float: right; position: relative; top: -1em;"></div>
</header>

<div id="contents">

<div id="product_profile" style="display: none;"></div>
<div id="download_queue" style="display: none;"></div>

<div id="leftColumn">
<div id="filters"></div>
</div>
<div id="rightColumn">
<div id="searchMap"></div>

<!-- loading async spinner -->
<div class="ui-widget">
  <div id="async-spinner" class="ui-state-highlight ui-corner-all">
    <p>
      <span class="ui-icon"></span>
      <strong>Loading search results&hellip;</strong>
    </p>
  </div>
</div>


<!-- results banner -->
<div class="ui-widget">
  <div id="results-banner" class="ui-state-highlight ui-corner-all"> 
    <p>
      <span class="ui-icon ui-icon-info"></span>
      <strong>No results were found</strong> for your current search criteria.
    </p>
  </div>
</div>

<!-- error banner -->
<div class="ui-widget">
  <div id="error-message" class="ui-state-error ui-corner-all"> 
    <p>
      <span class="ui-icon ui-icon-alert"></span>
      <strong>An error occurred</strong> while processing your current search (<code id="error-message-code"></code>).
    </p>
  </div>
</div>


<div id="results-widget-wrapper" class="ui-widget">
<table id="results"></table>
</div>
</div>
</div>

<footer>
<div id="centerer">
<div>
<h2>ASF * Alaska Satellite Facility</h2>
<ul>
<li><a href="/about">Location</a></li>
<li><a href="/contact">User Support Office</a></li>
<li><a href="/about">About Us</a></li>
</ul>
</div>

<div>
<h2>ASF Programs</h2>
<ul>
<li><a href="/program/sdc" title="">SAR Data Center</a></li>
<li><a href="/program/asfe" title="">ASF Enterprise</a></li>
<li><a href="/program/stgs" title="">Satellite Tracking Ground Station</a></li>
<li><a href="/program/gdc" title="">GeoData Center</a></li>
</ul>
</div>

<div>
<h2>ASF Support Resources</h2>
<ul>
<li><a href="/citation">Citation Information</a></li>
<li><a href="/contact">Contact Us</a></li>
<li><a href="/downloads/software_tools" title="">Software Tools</a></li>
<li><a href="/program/sdc/proposals" title="Submit a Proposal | Alaska Satellite Facility">Submit Proposal</a></li>
</ul>
</div>

<div>
<h2>Alaska Satellite Facility</h2>
<p>ASF Dataportal V[% c.config.version %]</p>
<p>
2011 &copy; Alaska Satellite Facility<br/>
Geophysical Institute<br/>
903 Koyukuk Dr.<br/>
P.O. Box 757320<br/>
Fairbanks, AK 99775-7320<br/>
Phone: (907) 474-6166<br/>
Fax: (907) 474-2665<br/>
Email: uso@asf.alaska.edu<br/>
</p>
</div>

</div>
</footer>
</div>

